{
  "name": "cucumberjs-protractor",
  "version": "1.0.0",
  "description": "To demostrate protractor cucumber tests for angular/non angular apps with allure report framework",
  "main": "index.js",
  "homepage": "https://github.com/iamsethi/cucumberjs-protractor",
  "keywords": [
    "protractor",
    "cucumber",
    "allure reports",
    "angular",
    "angularjs",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "posgreSQL",
    "gherkin",
    "automation testing"
  ],
  "scripts": {
    "test": "protractor config/config.js",
    "webdriver-start": "webdriver-manager start",
